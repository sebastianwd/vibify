{
  "name": "vibify",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "check": "biome check --write .",
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F @my-better-t-app/backend dev",
    "dev:setup": "turbo -F @my-better-t-app/backend dev:setup"
  },
  "dependencies": {},
  "devDependencies": {
    "turbo": "^2.5.4",
    "@biomejs/biome": "^2.2.0",
    "typescript": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "react": "catalog:",
      "react-dom": "catalog:",
      "@types/react": "catalog:",
      "@types/react-dom": "catalog:"
    }
  },
  "packageManager": "pnpm@10.17.1"
}
